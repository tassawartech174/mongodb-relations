{
  "name": "tassawartech174/mongodb-relations",
  "description": "Custom relations for MongoDB in Laravel",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "TassawarTech174\\MongodbRelations\\": "src/"
    }
  },
  "require": {
    "php": ">=7.4",
    "illuminate/database": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "TassawarTech174\\MongodbRelations\\MongodbRelationsServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "keywords": ["laravel", "mongodb", "relations", "Unidirectional Many To Many Relation"]
}
