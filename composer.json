{
  "name": "tassawartech174/mongodb-relations",
  "description": "Custom Laravel Eloquent relationships for MongoDB using jenssegers/mongodb and mongodb/laravel-mongodb. Supports unidirectional many-to-many and reverse lookups for embedded and reference-based relations.",
  "keywords": [
    "laravel",
    "mongodb",
    "eloquent",
    "relationships",
    "many-to-many",
    "custom relations",
    "mongodb eloquent relationships",
    "mongodb/laravel-mongodb",
    "jenssegers",
    "laravel mongodb relations",
    "laravel eloquent mongodb",
    "reverse relation mongodb",
    "laravel mongodb package",
    "unidirectional relationship"
  ],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "TassawarTech174\\MongodbRelations\\": "src/"
    }
  },
  "require": {
    "php": ">=7.4",
    "illuminate/database": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "TassawarTech174\\MongodbRelations\\MongodbRelationsServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
